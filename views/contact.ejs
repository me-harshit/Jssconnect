  <script type="module">
    import { annotate, annotationGroup  } from 'https://unpkg.com/rough-notation?module';
    const e = document.querySelectorAll('.myAnnotation');
    const e3 = document.querySelectorAll('.e3')
      setTimeout(() => {
      for (let i = 0; i < e.length; i++) {
        const annotation = annotate(e[i], { type: 'underline', multiline: true ,color: 'yellow' });
        const a3 = annotate(e3[i], { type: 'box',color: 'crimson' });
        const ag = annotationGroup([a3, annotation]);
        ag.show();
      }
      }, 1000);
  </script>

