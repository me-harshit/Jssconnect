<%- include("header") %>
  <style>
    #container {
      margin-top: 40px;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
    }

    #goal{
      position: relative;
      width: 100%;
      color: white;
      background-color: rgba(0, 0, 0, 0.7);
      height: 540px;
    }
    
    #goal::before{
      content: "";
      position: absolute;
      background: url('https://jssaten.ac.in/images/JSS-Main-Gate.jpg') center center/cover no-repeat;
      width: 100%;
      height: 100%;
      top: 0;left: 0;
      z-index: -10;
    }

    #container form {
      width: 45%;
    }

    a {
      color: black;
    }

    @media screen and (max-width: 600px) {
      #container {
        padding: 10px;
        text-align: center;
      }

      #container form {
        width: 81%;
      }
    }

    @media screen and (max-width: 1000px) {
      #logoText {
        display: none;
      }
    }

    .memberName:hover {
      color: orangered;
    }

  

    .socials {
      font-size: 2rem;
      margin: 10px;
    }

    #newsBtn {
      border-radius: 15px;
      padding: 7px;
      font-size: 1.2rem;
      border: 2px solid white;
      cursor: pointer;
      background-color: #434e52;
      color: white;
    }

    #newsBtn:hover {
      filter: saturate(10);
      /* background-color: aqua; */
    }

    .socials:hover {
      filter: saturate(3);
    }

    .myFloatingBtn:hover {
      transform: rotate(360deg);
      transition: 0.7s linear all;
    }

    .day {
      display: none;
    }
  </style>
</head>

